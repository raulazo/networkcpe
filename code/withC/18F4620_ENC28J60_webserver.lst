0118C:  MOVLW  0A
0118E:  MOVLB  3
01190:  MOVWF  x85
01192:  MOVLB  0
01194:  RCALL  10E8
01196:  MOVFF  01,384
0119A:  MOVLW  00
0119C:  MOVLB  3
0119E:  BTFSC  01.0
011A0:  MOVLW  01
